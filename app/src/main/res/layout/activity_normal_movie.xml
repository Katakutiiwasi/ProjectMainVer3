<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_normal_movie"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.b1014100_2.projectmainver3.normalmovie.NormalMovieActivity">
    <VideoView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:id="@+id/videoView" />
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/n_movie_bg"
        android:background="@color/colorAccent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.b1014100_2.moviesample.MainActivity">
        <Button
            android:text="back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/n_movie_backbutton"
            android:layout_alignStart="@+id/n_movie_backbutton"
            android:layout_marginTop="68dp"
            android:id="@+id/n_movie_replaybutton" />
        <Button
            android:text="replay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:id="@+id/n_movie_backbutton"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="50dp" />
    </RelativeLayout>
</RelativeLayout>
